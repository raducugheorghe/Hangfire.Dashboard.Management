{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ConnectionStrings": {
    "HangfireConnection": "Server=(localdb)\\mssqllocaldb;Database=hangfire;AttachDBFilename=|DataDirectory|\\hangfire.mdf;Trusted_Connection=True;MultipleActiveResultSets=true;Integrated Security=SSPI"
  },
  "log4net": {
    "ConfigFilePath": "log4net.config.json",
    "Watch": true
  }
}
